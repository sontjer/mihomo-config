profile:
  store-selected: true
  store-fake-ip: true

sniffer:
  enable: true
  sniff:
    HTTP:
      ports: [80, 8080-8880]
      override-destination: true
    TLS:
      ports: [443, 8443]
    QUIC:
      ports: [443, 8443]
  skip-domain:
    - "Mijia Cloud"
    - "+.push.apple.com"

tun:
  enable: true
  stack: mixed
  dns-hijack:
    - "any:53"
    - "tcp://any:53"
  auto-route: true
  auto-redirect: true
  auto-detect-interface: true

dns:
  enable: true
  ipv6: true
  respect-rules: true
  enhanced-mode: fake-ip
  fake-ip-range: 28.0.0.1/8
  fake-ip-filter:
#    - "*"
    - "+.lan"
    - "+.local"
    - "+.market.xiaomi.com"
    - geosite:private
    - geosite:cn
  nameserver:
    - 192.168.2.4
  proxy-server-nameserver:
    - 192.168.2.4
  nameserver-policy:
    "rule-set:cn_domain,private_domain":
    - 192.168.2.4
    "rule-set:geolocation-!cn":
    - 192.168.2.4
    
